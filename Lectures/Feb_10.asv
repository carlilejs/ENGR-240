clear
clc

choice = input ('Do you want to see a contour plot mesh? [C/M]: ', 's');

x = linspace (-pi, pi, 20);
y = linspace (-10, 16, 30);

[XX, YY] = meshgrid (x, y);

ZZ = XX.^2 + YY.^2;


switch choice
    case 'C'
        contour (XX, YY, ZZ);
    case 'M'
        mesh (XX, YY, ZZ);
    otherwise
        
end